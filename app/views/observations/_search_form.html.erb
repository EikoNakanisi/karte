<%= render layout: 'shared/search_box' do %>

<div class="row">
<div class="col-sm-6 col-sm-offset-3">
  <div class="form-group">
    <label class="col-sm-4 control-label" for="">患者ＩＤ</label>
    <div class="col-sm-8">
      <%= f.text_field :pt_no_start, class: 'form-control' %>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label" for="">観察名</label>
    <div class="col-sm-8">
      <%= f.text_field :ob_name_start, class: 'form-control' %>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label" for="">category含まれる文字列</label>
    <div class="col-sm-8">
      <%= f.text_field :category_cont, class: 'form-control' %>
    </div>
  </div>
</div>

<div class="col-sm-12 row text-center">
  <%= link_to observations_path, class: 'btn btn-default btn-left' do %>クリア<% end %>
  <button class="btn btn-default btn-primary" type="submit"><i class="fa fa-search"></i> 検索</button>
</div>

</div>

<% end %>









